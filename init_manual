#!/bin/bash

#### Config
./set 0x00 0x88 # CR1
./set 0x01 0x11 # CR2
./set 0x02 0xb0 # CR3
./set 0x03 0xff # FFDC (Default)
./set 0x04 0xef # MASK
./set 0x05 0x18 # IFR (Default)
./set 0x06 40   # RHSH degC
./set 0x07 00   # RHSL
./set 0x08 50   # LOTSH degC
./set 0x09 00   # LOTSL
./set 0x0a 40   # ROTSH degC
./set 0x0b 00   # ROTSL
./set 0x0c 50   # LHSH degC
./set 0x0d 00   # LHSL
./set 0x0e 0xff # TCTH (Default)
./set 0x0f 0xff # TCTL (Default)

### User EEPROM
./set 0x10 0xba # Signature
./set 0x11 0x00 # Type: Manual Control
./set 0x12 0x00 # Version
./set 0x13 0x00
./set 0x14 0x00
./set 0x15 0x00
./set 0x16 0x00
./set 0x17 0x00

### LUT
# Left alone.

### Operational
# Set Duty Cycle
./set 0x50 0x00 # PWMR
